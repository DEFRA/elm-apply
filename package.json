{
  "name": "land-management-plan-web",
  "version": "1.0.0",
  "description": "ELM Land Management Plan web front end",
  "main": "index.js",
  "homepage": "https://github.com/DEFRA/elm-land-management-plan-web",
  "scripts": {
    "build": "npm run build:css",
    "build:css": "node-sass --output-style=expanded --output=server/public/build/stylesheets client/sass",
    "pretest": "npm run test:lint",
    "start": "node index.js",
    "test": "npm run test:unit",
    "test:lint": "standard",
    "test:pa11y": "pa11y-ci",
    "test:snyk": "snyk test",
    "test:unit": "lab -c -r console -o stdout -r junit -o ./test-output/TEST-$(date '+%Y%m%d-%H%M%S').xml"
  },
  "author": "Defra DDTS",
  "contributors": [
    "Matthew Collins <matthew.collins@defra.gov.uk>",
    "David Bingham <david.bingham@defra.gov.uk>"
  ],
  "license": "OGL-UK-3.0",
  "dependencies": {
    "boom": "7.3.0",
    "govuk-frontend": "2.11.0",
    "hapi": "18.1.0",
    "inert": "5.1.3",
    "joi": "14.3.1",
    "node-fetch": "^2.5.0",
    "nunjucks": "3.2.0",
    "vision": "5.4.4"
  },
  "devDependencies": {
    "blipp": "^4.0.0",
    "code": "5.2.4",
    "codeclimate-test-reporter": "0.5.1",
    "good": "8.1.2",
    "good-console": "8.0.0",
    "good-file": "6.0.1",
    "good-squeeze": "5.1.0",
    "lab": "^18.0.2",
    "node-sass": "^4.11.0",
    "snyk": "^1.154.0",
    "standard": "12.0.1"
  }
}
